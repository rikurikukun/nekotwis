
<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <li class="media">
        <div class="media-left">
          
          <img class="media-object img-rounded" src="<%= user.picture_url %>" alt="">
           
        </div>
        <div class="media-body">
          
          <div class="media-font">
            <p><%= link_to user.name, user_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>