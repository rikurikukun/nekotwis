      <li><%= link_to followers_user_path(@user) do %> <img src="/images/logo73.png" alt="フォロワー"><span class="active"></span><% end %></li>
      <li><%= link_to favorites_user_path(@user) do %> <img src="/images/logo74.png" alt="フォロワー"><span class="active"></span><% end %></li>
    </ul>
    <%= render 'users', users: @followings %>
   
  </div>
</div>