<div class="row">
  <aside class="col-xs-4">
   
      <div class="panel-heading">
      </div>
      <div class="panel-body">
         <img class="panel-pic" src="<%= @user.picture %>" alt="">　
       <img src="/images/namae2.png" alt="なまえ">
        <h4><%= @user.name %></h4>
       <img src="/images/profile3.png" alt="じこしょうかい">
