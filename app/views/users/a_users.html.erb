<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <li class="media">
        <div class="media-left">
           <img class="media-object img-rounded" src="<%= users.picture_url %>" alt="">
          <%= users.content %>
        <div>
        <div class="media-body">
          <div>
            <%= users.name %>
          </div>
          <div>
            <p><%= link_to 'View profile', users_path(user) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>